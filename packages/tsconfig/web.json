{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web Config",
  "extends": "./base.json",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "noEmit": true,
    "target": "es5"
  },
  "include": ["src", "remix-env.d.ts"],
  "exclude": ["node_modules"]
}
